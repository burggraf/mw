{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "display",
  "description": "Capabilities for display output windows",
  "windows": ["display-*"],
  "permissions": [
    "core:default",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "store:default",
    "store:allow-load",
    "store:allow-save",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "store:allow-keys"
  ]
}
