{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:window:allow-maximize",
    "core:window:allow-minimize",
    "core:window:allow-close",
    "opener:default",
    "store:default",
    "store:allow-keys",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "store:allow-save",
    "process:default"
  ]
}
